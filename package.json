{
  "name": "pug-line-parser",
  "version": "0.1.0",
  "description": "Jison generated Pug parser",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "author": "Adam Koch (aakoch)",
  "license": "MIT",
  "scripts": {
    "preinstall": "npx npm-force-resolutions",
    "test_es": "npm run build_es && DEBUG='pug-line-lexer' node dist/index.mjs",
    "build_es": "scripts/build_es.sh",
    "test_common": "npm run build_common && DEBUG='pug-line-lexer' node dist/index.cjs",
    "build_common": "scripts/build_common.js",
    "build": "scripts/build.sh",
    "watch": "fswatch -0ou src/index.jison | xargs -t -0 -n1 /bin/zsh -c $PWD/rebuild.sh"
  },
  "keywords": [
    "stream",
    "transformer",
    "lexer",
    "parser",
    "pug",
    "foo-dog",
    "foodog",
    "foo dog"
  ],
  "dependencies": {
    "@aakoch/utils": "file:../utils",
    "foo-dog-attrs": "file:../foo-dog-attrs",
    "@gerhobbelt/ast-util": "^0.6.1-4",
    "@gerhobbelt/recast": "^0.15.1-32",
    "@jaredpalmer/wrapline": "^2.0.2",
    "command-line-args": "^5.2.0",
    "command-line-usage": "^6.1.1",
    "concat-stream": "^2.0.0",
    "debug": "^4.3.2",
    "deep-filter": "^1.0.2",
    "hasha": "^5.2.2",
    "indent-transformer": "^0.2.1",
    "jison-gho": "github:GerHobbelt/jison",
    "pretty-format": "^27.2.0",
    "replace-in-file": "^6.3.1",
    "through2-objectify": "^0.1.1"
  },
  "devDependencies": {
    "chai": "^4.3.4",
    "dyp": "^1.0.1",
    "filecompare": "^1.0.4",
    "jest": "^27.2.0",
    "mocha": "^9.1.1",
    "npm-force-resolutions": "^0.0.10",
    "nyc": "^15.1.0"
  },
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.cjs"
  },
  "resolutions": {
    "minimist": "1.2.5"
  },
  "type": "module"
}
