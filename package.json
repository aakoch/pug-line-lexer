{
  "name": "pug-line-lexer",
  "version": "0.1.0",
  "description": "Jison generated Pug parser",
  "source": "src/index.js",
  "module": "dist/module.js",
  "author": "Adam Koch (aakoch)",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf build && rm -rf dist",
    "preinstall": "npx npm-force-resolutions",
    "test": "scripts/preprocess.sh && DEBUG='*' node test/main.spec.js && DEBUG='*' node test/code.spec.js",
    "build": "scripts/preprocess.sh && parcel build",
    "rebuild": "scripts/preprocess.sh && parcel build",
    "watch": "fswatch -0ou src/index.jison | xargs -t -0 -n1 /bin/zsh -c $PWD/rebuild.sh"
  },
  "keywords": [
    "stream",
    "transformer",
    "lexer",
    "parser",
    "pug",
    "foo-dog",
    "foodog",
    "foo dog"
  ],
  "dependencies": {
    "@foo-dog/attrs": "^0.2.2",
    "@foo-dog/utils": "^0.2.2",
    "@gerhobbelt/ast-util": "^0.6.1-4",
    "@gerhobbelt/recast": "^0.15.1-32",
    "@jaredpalmer/wrapline": "^2.0.2",
    "command-line-args": "^5.2.0",
    "command-line-usage": "^6.1.1",
    "concat-stream": "^2.0.0",
    "debug": "^4.3.2",
    "deep-filter": "^1.0.2",
    "hasha": "^5.2.2",
    "indent-transformer": "^0.2.1",
    "jison-gho": "github:GerHobbelt/jison",
    "lodash": "^4.17.21",
    "pretty-format": "^27.2.0",
    "replace-in-file": "^6.3.1",
    "through2-objectify": "^0.1.1"
  },
  "devDependencies": {
    "chai": "^4.3.4",
    "dyp": "^1.0.1",
    "filecompare": "^1.0.4",
    "jest": "^27.2.0",
    "mocha": "^9.1.1",
    "npm-force-resolutions": "^0.0.10",
    "nyc": "^15.1.0",
    "parcel": "^2.0.1",
    "tap": "^15.1.5"
  },
  "targets": {
    "module": {
      "optimize": false,
      "isLibrary": true
    }
  },
  "engines": {
    "node": ">=16"
  },
  "exports": {
    "import": "./dist/module.js"
  },
  "resolutions": {
    "minimist": "1.2.5"
  },
  "type": "module"
}
